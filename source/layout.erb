<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
    <%= favicon_tag 'favicon.ico' %>
    <link rel="alternate" type="application/atom+xml" title="Atom feed" href="/feed.xml" />

    <%= stylesheet_link_tag 'bootstrap.min' %>
    <%= stylesheet_link_tag 'bootstrap-responsive' %>
    <%= stylesheet_link_tag 'style-apr1' %>
    <%= stylesheet_link_tag 'highlight' %>
    <%= javascript_include_tag 'highlight.pack' %>
    <link href='http://fonts.googleapis.com/css?family=Paytone+One|Ubuntu:400,700,300italic' rel='stylesheet' type='text/css'>

    <title><% if defined?(current_page) and current_page.respond_to?(:title) %><%= current_page.title %> - <% end %>Vaibhavkhl</title>

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-2578052-2']);
      _gaq.push(['_trackPageview']);

      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();

    </script>

  </head>
  <body>

    <div class='container-fluid'>
        <div class='row-fluid'>
          <div class='span9 content'>
            <%= yield %>
          </div>
          <div class='span3'>
            <section class='sidebar'>
              <a href='/'><%= image_tag 'vaibhav.jpg', class: 'avatar', alt: 'Evil Trout Cartoon' %></a>

              <h1>Vaibhav Kohli</h1>

              <section class='about'>
                <p>I'm a ruby on rails developer.</p>
                
                <p>I live in Mumbai. I like web development.</p>
                
              </section>

            </section>
          </div>

        </div>
      </div>



  </body>

  <script>
    $(document).ready(function() {
      $('pre code').each(function(i, e) {hljs.highlightBlock(e)});
    });
  </script>

</html>
